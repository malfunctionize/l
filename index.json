{
  "install": [{
    "method": "shell.run",
    "params": {
      "message": "git clone https://github.com/ggerganov/llama.cpp",
      "path": "."
    }
  }, {
    "method": "shell.run",
    "params": {
      "message": "mkdir build",
      "path": "./llama.cpp"
    }
  }, {
    "method": "shell.run",
    "params": {
      "message": "cmake ..",
      "path": "./llama.cpp/build"
    }
  }, {
    "method": "shell.run",
    "params": {
      "message": "cmake --build . --config Release",
      "path": "./llama.cpp/build"
    }
  }],
  "gpt4all": [{
    "method": "download",
    "params": {
      "url": "{{models[2].items[0].url}}",
      "path": "./llama.cpp/models/{{models[2].name}}-{{models[2].items[0].name}}"
    }
  }]
}
